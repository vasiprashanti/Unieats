{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/admin/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/index.html"
    }
  ],
  "outputDirectory": "dist"
}
